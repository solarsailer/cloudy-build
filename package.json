{
  "name": "cloudy-build",
  "version": "0.4.0",
  "description": "Quick access interface for Unity Cloud Build.",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/solarsailer/cloudy-build/",
  "author": {
    "name": "Matthieu Oger",
    "url": "https://solarsailer.net/"
  },
  "keywords": [],
  "scripts": {
    "start": "nodemon server/index.js",
    "build": "next build",
    "prod": "next build && NODE_ENV=production node server/index.js"
  },
  "dependencies": {
    "@zeit/next-typescript": "1.1.0",
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "next": "^6.1.1",
    "polished": "^1.9.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "styled-components": "^3.3.3"
  },
  "devDependencies": {
    "@types/next": "latest",
    "@types/react": "^16.4.7",
    "babel-plugin-styled-components": "^1.5.1",
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.3"
  },
  "babel": {
    "presets": [
      "next/babel",
      "@zeit/next-typescript/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  }
}
